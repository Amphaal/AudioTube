{
    "configurations": [
        {
            "name": "Win32",
            "browse": {
                "path": [
                    "${env:MINGW64_ROOT}/include/c++",
                    "${env:MINGW64_ROOT}/include",
                    "${env:MINGW64_ROOT}/lib"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/audiotube",
                "${workspaceFolder}/audiotube/include",
                "${workspaceFolder}/audiotube/include/audiotube",
                "${workspaceFolder}/audiotube/promise-cpp",
                "${workspaceFolder}/audiotube/nlohmann_json/single_include",
                "${workspaceFolder}/_genDebug/audiotube/pcre2",
                "${workspaceFolder}/audiotube/asio/asio/include",
                "${workspaceFolder}/audiotube/jpcre2/src"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "SPDLOG_FMT_EXTERNAL"
            ],
            "compilerPath": "${env:MINGW64_ROOT}/bin/clang++.exe",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "clang-x64"
        },
        {
            "name": "MacOS",
            "cStandard": "c11",
            "cppStandard": "c++20",
            "intelliSenseMode": "clang-x64",
            "includePath": [
                "${default}",
                "${workspaceFolder}/**"
            ]
        }
    ],
    "version": 4
}